pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git "https://github.com/Deep-anjali101/HelloMaven.git"
            }
        }
        stage('build') {
            steps {
                bat "mvn clean test "
            }
        }
        stage('deploy') {
            steps {
                bat "mvn package"
            }
        }
        
    }
}